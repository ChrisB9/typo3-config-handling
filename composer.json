{
    "name": "helhum/typo3-config-handling",
    "description" : "Simple but powerful configuration handling for TYPO3 CMS",
    "license": "GPL-2.0-or-later",
    "require": {
        "php": ">=7.2 <7.3",
        "ext-json": "*",
        "typo3/cms-core": "~9.5.0",
        "helhum/config-loader": ">=0.9 <0.12",
        "helhum/typo3-console": "^5.5",
        "symfony/console": "^4.0",
        "symfony/yaml": "^4.0",
        "typo3/cms-composer-installers": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "composer/composer": "^1.6",
        "mikey179/vfsStream": "^1.6"
    },
    "suggest": {
        "typo3/cms-install": "If you want to update distribution configuration with Extension Manager or Install Tool UI",
        "typo3-console/composer-typo3-auto-install": "For TYPO3 installation / setup during composer install"
    },
    "autoload": {
        "psr-4": {
            "Helhum\\TYPO3\\ConfigHandling\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Helhum\\TYPO3\\ConfigHandling\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "typo3/cms": {
            "app-dir": ".Build",
            "web-dir": ".Build/public"
        }
    }
}
